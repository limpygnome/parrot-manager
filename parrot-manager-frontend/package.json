{
  "name": "parrot-manager",
  "version": "1.0.0",
  "scripts": {
    "pre-install": "npm i -g @angular/cli npm-check-updates",
    "start": "ng serve --source-map=false",
    "start-prod": "ng serve --prod --source-map=false",
    "build": "ng build --source-map=false",
    "build-prod": "ng build --prod --source-map=false",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "updates": "ncu",
    "snyk-protect": "snyk protect",
    "prepare": "npm run snyk-protect"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/limpygnome/parrot-manager/blob/master/LICENSE"
    }
  ],
  "dependencies": {
    "@angular/common": "6.1.4",
    "@angular/compiler": "6.1.4",
    "@angular/core": "6.1.4",
    "@angular/forms": "6.1.4",
    "@angular/http": "6.1.4",
    "@angular/platform-browser": "6.1.4",
    "@angular/platform-browser-dynamic": "6.1.4",
    "@angular/router": "6.1.4",
    "@angular/upgrade": "6.1.4",
    "angular-in-memory-web-api": "0.6.1",
    "bootbox": "4.4.0",
    "bootstrap": "4.1.3",
    "core-js": "2.5.7",
    "cuint": "0.2.2",
    "jquery": "3.3.1",
    "jstree": "3.3.6",
    "npm": "^6.4.0",
    "reflect-metadata": "0.1.12",
    "rxjs": "6.2.2",
    "systemjs": "0.21.4",
    "toastr": "2.1.4",
    "zone.js": "0.8.26",
    "snyk": "^1.230.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.7.0",
    "@angular/cli": "6.1.4",
    "@angular/compiler-cli": "6.1.4",
    "@types/core-js": "2.5.0",
    "@types/node": "10.7.1",
    "concurrently": "3.6.1",
    "lite-server": "2.4.0",
    "typescript": "2.7.2"
  },
  "snyk": true
